
{
    "name": "Cursor Tracker",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["storage", "tabs"]
    ,
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon_red.png",
        "24": "icon_red.png",
        "32": "icon_red.png"
      },
    "icons": {
      "16": "icon_red.png",
      "24": "icon_red.png",
      "32": "icon_red.png"
    }
  }
  }
  